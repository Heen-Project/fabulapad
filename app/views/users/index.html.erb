<div class="card">
    <div class="card-header">
        <h2 class="text-center">Fabulapad writers</h2>
    </div>
    <div class="card-body">
        <%= render 'shared/search_bar', path: users_path %>
        <%= render 'users', obj: @users %>
        <div class="flickr_pagination mt-3">
            <%= will_paginate @users, :container => false %>
        </div>
    </div>
    <%= render 'shared/return_home_footer', obj: nil %>
</div>